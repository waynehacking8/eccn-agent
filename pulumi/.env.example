# AWS Deployment Configuration
# Copy this file to .env and fill in your values

# AWS Account Configuration
AWS_ACCOUNT_ID=your-aws-account-id
AWS_REGION=us-east-1

# AWS Custom Credentials (instead of IAM roles)
AWS_ACCESS_KEY_ID_CUSTOM=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY_CUSTOM=your-aws-secret-access-key

# OpenSearch Configuration
OPENSEARCH_ENDPOINT=https://search-eccn-agent-main-domain-xxx.us-east-1.es.amazonaws.com

# Model Configuration
EMBEDDING_MODEL_ID=cohere.embed-multilingual-v3
DEFAULT_BEDROCK_MODEL_ID=us.anthropic.claude-3-7-sonnet-20250219-v1:0

# Lambda Configuration
LOG_LEVEL=DEBUG